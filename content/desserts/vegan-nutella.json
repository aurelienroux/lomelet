  "instructions": [
    {
      "sectionTitle": "preparation",
      "sectionInstructions": [
        "Preheat the oven to 350°F (180°C).",
        "Gives 24 tbsp"
      ]
    },
    {
      "sectionTitle": "hazelnuts",
      "sectionInstructions": [
        "Spread the hazelnuts onto a baking tray and bake for 10-12 minutes, until lightly golden and fragrant, stirring once halfway.",
        "When cooled, pick up the hazelnuts and rub firmly between the palms of your hands so the skin falls off.",
        "Place the peeled hazelnuts in a high-speed food processor.",
        "Blend on high for 4 - 5 minutes, until a thin nut butter consistency is formed."
      ]
    },
    {
      "sectionTitle": "general",
      "sectionInstructions": [
        "Add the remaining ingredients and blend again for an additional 2 - 3 minutes until smooth and well incorporated.",
        "Store in an airtight jar in the fridge."
      ]
    }
